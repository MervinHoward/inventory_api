{
    "version": 2,
    "public": true,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/_next/static/(.*)",
            "dest": "public/_next/static/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "public/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "public/robots.txt"
        },
        {
            "src": "/web.config",
            "dest": "public/web.config"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "0",
        "APP_URL": "https://inventory-api.vercel.app",
        "APP_KEY": "@app_key",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "@db_host",
        "DB_PORT": "@db_port",
        "DB_DATABASE": "@db_database",
        "DB_USERNAME": "@db_username",
        "DB_PASSWORD": "@db_password",
        "CACHE_DRIVER": "array",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr"
    }
}
